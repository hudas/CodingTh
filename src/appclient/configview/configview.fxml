<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane fx:id="root" alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="appclient.configview.ConfigViewController">
   <children>
   
       <Text text="Demonstracijos konfigūracija" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="0" />
   
       <Label fx:id="errorLabel" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
   
       <Text text="Kodo ilgis - n" GridPane.columnIndex="0" GridPane.halignment="RIGHT" GridPane.rowIndex="2" />
       <TextField fx:id="codeLength" promptText="Kodo ilgis" GridPane.columnIndex="1" GridPane.rowIndex="2" />
   
       <Text text="Kodo dimensija - k" GridPane.columnIndex="0" GridPane.halignment="RIGHT" GridPane.rowIndex="3" />
       <TextField fx:id="codeDimension" promptText="Kodo dimensija" GridPane.columnIndex="1" GridPane.rowIndex="3" />
   
       <Text text="Generuojanti matrica" GridPane.columnIndex="0" GridPane.halignment="RIGHT" GridPane.rowIndex="4" GridPane.valignment="TOP" />
       <TextArea fx:id="generatorMatrix" prefColumnCount="20" prefRowCount="5" promptText="Generuojanti matrica" GridPane.columnIndex="1" GridPane.rowIndex="4" />
       <Button fx:id="generate" text="Generuoti" GridPane.columnIndex="3" GridPane.rowIndex="4" GridPane.halignment="LEFT" GridPane.valignment="TOP"/>
       <Text text="Generuojančią matricą pateikite kaip reiškmių skirtuką naudodami kablelį, o eilučių kabliataškį." GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="5" />
   
       <Text text="Kanalo iškraipymų tikimybė" GridPane.columnIndex="0" GridPane.halignment="RIGHT" GridPane.rowIndex="6" />
       <TextField fx:id="channelNoise" promptText="Iškraipymų tikimybė" GridPane.columnIndex="1" GridPane.rowIndex="6" />
   
       <Text text="Dimensiją ir ilgi pateikti būtina, kitus atributus sugeneruos automatiškai" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="7" />
       <Button fx:id="submit" text="Išsaugoti" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="8" />

   </children>
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>

</GridPane>
